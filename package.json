{
  "name": "e2e",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "run-s build run",
    "build-ts": "protoc --plugin=\"protoc-gen-ts=node_modules/.bin/protoc-gen-ts\" --js_out=\"import_style=commonjs,binary:.\" --ts_out=\".\" e2e/protos/readinglist.proto",
    "clean-ts": "rm -rf e2e/protos/{*.ts,*.js}",
    "clean-state": "rm -rf e2e/protos/BasicState",
    "clean" : "yarn run clean-ts && yarn run clean-state",
    "build": "yarn run build-ts && mkdir -p e2e/protos/BasicState && protoc -I. -I./e2e/protos -I./state/options.proto --plugin=./protoc-gen-state --state_out=e2e/protos/BasicState ./e2e/protos/basic.proto",
    "run": "yarn run build && echo 'should start a backend server' && parcel src/index.tsx",
    "test": "JASMINE_CONFIG_PATH=e2e/spec/support/jasmine.json node_modules/.bin/ts-node --node_options=--preserve-symlinks --node_options=--max-old-space-size=8192 node_modules/jasmine/bin/jasmine"
  },
  "devDependencies": {
    "@types/google-protobuf": "3.2.7",
    "@types/jasmine": "2.8.7",
    "@types/lodash": "4.14.110",
    "@types/node": "10.5.0",
    "@types/prop-types": "15.5.6",
    "@types/react": "^16.4.6",
    "@types/react-dom": "16.0.6",
    "@types/react-redux": "5.0.21",
    "@types/rx": "4.1.1",
    "ansi-html": "^0.0.7",
    "concurrently": "^4.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "html-entities": "^1.2.1",
    "jasmine": "3.1.0",
    "jasmine-enzyme": "6.0.2",
    "jsdom": "^13.0.0",
    "module-alias": "2.1.0",
    "npm-run-all": "4.1.3",
    "object-assign": "^4.1.1",
    "parcel-bundler": "^1.10.3",
    "promise": "^8.0.2",
    "protobufjs": "6.8.6",
    "raf": "^3.4.1",
    "redux-devtools-extension": "2.13.5",
    "thread-loader": "^1.2.0",
    "ts-loader": "4.5.0",
    "ts-node": "7.0.0",
    "ts-protoc-gen": "0.6.0",
    "tsickle": "0.32.1",
    "tslint": "5.11.0",
    "tslint-config-airbnb": "5.11.0",
    "tslint-react": "3.6.0",
    "typescript": "^3.2.1",
    "url-polyfill": "^1.1.0",
    "whatwg-fetch": "^3.0.0"
  },
  "dependencies": {
    "google-protobuf": "3.5.0",
    "grpc-web-client": "0.6.3",
    "lodash": "4.17.10",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-redux": "5.0.7",
    "redux": "3.7.2",
    "redux-observable": "0.18.0",
    "redux-saga": "0.16.0",
    "rxjs": "5.5.6",
    "typesafe-actions": "2.0.4"
  }
}
